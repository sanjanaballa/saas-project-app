<p id="notice"><%= notice %></p>

<h1>Arifacts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Key</th>
      <th>Project</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @arifacts.each do |arifact| %>
      <tr>
        <td><%= arifact.name %></td>
        <td><%= arifact.key %></td>
        <td><%= arifact.project %></td>
        <td><%= link_to 'Show', arifact %></td>
        <td><%= link_to 'Edit', edit_arifact_path(arifact) %></td>
        <td><%= link_to 'Destroy', arifact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Arifact', new_arifact_path %>
